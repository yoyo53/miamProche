<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProducteurActivity" >

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <ImageView
        android:id="@+id/userImage"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/logo"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        android:contentDescription="Photo du producteur" />
    <ScrollView
        android:id="@+id/scrollview_greeting"
        android:layout_height="fill_parent"
        android:layout_width="fill_parent"
        app:layout_constrainedWidth="true"
        app:layout_constrainedHeight="true"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintStart_toEndOf="@id/userImage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/userImage"
        app:layout_constraintBottom_toBottomOf="@id/userImage"
        app:layout_constraintVertical_bias="0.5"
        app:layout_constraintHorizontal_bias="0" >

        <TextView
            android:id="@+id/main_textview_greeting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:justificationMode="inter_word"
            android:textSize="15sp" />
    </ScrollView>

    <EditText
        android:id="@+id/editText_nom_produit"
        android:layout_width="0dp"
        app:layout_constraintWidth_percent="0.4"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:textStyle="italic"
        android:hint="Nom du produit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/userImage"
        app:layout_constraintHorizontal_bias="0.05"
        android:importantForAutofill="no"
        android:inputType="text|textCapSentences"
        android:nextFocusDown="@+id/editText_quantite" />

    <EditText
        android:id="@+id/editText_quantite"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:textStyle="italic"
        android:hint="QuantitÃ©"
        app:layout_constraintStart_toEndOf="@+id/editText_nom_produit"
        app:layout_constraintEnd_toStartOf="@+id/editText_prix"
        app:layout_constraintTop_toBottomOf="@id/userImage"
        android:importantForAutofill="no"
        android:inputType="text|textCapSentences"
        android:nextFocusDown="@+id/editText_prix" />

    <EditText
        android:id="@+id/editText_prix"
        android:layout_width="0dp"
        app:layout_constraintWidth_percent="0.25"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:textStyle="italic"
        android:hint="Prix"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/userImage"
        app:layout_constraintHorizontal_bias="0.95"
        android:importantForAutofill="no"
        android:inputType="text|textCapSentences"
        android:nextFocusDown="@+id/editText_description" />

    <EditText
        android:id="@+id/editText_description"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:textStyle="italic"
        android:hint="Description"
        app:layout_constraintStart_toStartOf="@id/editText_nom_produit"
        app:layout_constraintEnd_toEndOf="@id/editText_nom_produit"
        app:layout_constraintTop_toBottomOf="@id/editText_nom_produit"
        android:importantForAutofill="no"
        android:inputType="text|textCapSentences" />

